package com.job.assignment1;
        import java.util.*;
  public class palindrome
       {
		public static void main(String[] args) 
	       {
		int n,rem,rev=0;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		int x=n;
		if(n>0)
		{
		while(n!=0)
		{
			rem=n%10;
			rev=rev*10+rem;
			n=n/10;
		}
		if(x==rev)
		{
			System.out.println("true");
		}
		else
		{
			System.out.println("false");
		}
		}
		else
		{
			System.out.println("false");
		}
	     }
        }
