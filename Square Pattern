package com.job.assignment1;
import java.util.*;
public class patternsquare 
{
	public static void main(String[] args) {
		int n,i,j;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		int num=1;
		for(i=1;i<n;i++)
		{
			for(j=1;j<n;j++)
			{
				if(i>=j)
				{
					System.out.print(" "+num*num);
					num=num+1;
				}
			}
			System.out.println();
		}
	}
}
